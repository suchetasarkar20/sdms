<h1> Welcome</h1>
<div class="block" id="block-text">  
  <div class="content">
    <h2 class="title">Mails</h2>
    <div class="inner">
		<%@mailsReceived.each do |mail| %>
		<tr>
			<td><%= mail.from %>: <%=mail.subject%> </td> <br>
			<td><%= mail.description %> </td>
		</tr>
	<% end %>
    </div>
  </div>
</div>

<% content_for :sidebar, render(:partial => 'sidebar') -%>